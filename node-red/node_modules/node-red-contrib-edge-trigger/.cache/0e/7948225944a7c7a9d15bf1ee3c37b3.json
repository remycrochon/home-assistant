olumn":2},"generated":{"line":61,"column":15}},{"source":"hooks/useWidgets.tsx","name":null,"original":{"line":59,"column":2},"generated":{"line":62,"column":0}},{"source":"hooks/useWidgets.tsx","name":null,"original":{"line":59,"column":9},"generated":{"line":62,"column":9}},{"source":"hooks/useWidgets.tsx","name":null,"original":{"line":59,"column":10},"generated":{"line":62,"column":10}},{"source":"hooks/useWidgets.tsx","name":null,"original":{"line":59,"column":9},"generated":{"line":62,"column":17}},{"source":"hooks/useWidgets.tsx","name":null,"original":{"line":59,"column":19},"generated":{"line":62,"column":19}},{"source":"hooks/useWidgets.tsx","name":null,"original":{"line":59,"column":9},"generated":{"line":62,"column":33}},{"source":"hooks/useWidgets.tsx","name":null,"original":{"line":59,"column":2},"generated":{"line":62,"column":34}},{"source":"hooks/useWidgets.tsx","name":null,"original":{"line":60,"column":1},"generated":{"line":63,"column":0}}],"sources":{"hooks/useWidgets.tsx":"import { useReducer, useCallback, useState, useLayoutEffect } from 'react'\nimport { useSocket, useSocketWidgetsInitEvent, useSocketWidgetUpdateEvent, IWidget } from './useSocket'\n\nexport type TWidgetsActionType = 'update' | 'clear' | 'set'\nexport type TWidgetAction = {\n  type: TWidgetsActionType,\n  payload: any\n}\n\nfunction WidgetReducer(state : Array<IWidget>, action : TWidgetAction) {\n  switch(action.type) {\n    case 'update':\n      return state.map((oldWidget) => {\n        if (oldWidget.node.id === action.payload.node.id) {\n          return action.payload\n        } else {\n          return oldWidget\n        }\n      })\n    case 'clear':\n      return []\n    case 'set':\n      return action.payload\n    default:\n      throw `Implement action: ${action.type}`\n  }\n}\n\nexport default function uswWidgets(screenId : string) {\n  const [loaded, setLoaded] = useState(false)\n  const io = useSocket()\n  const [widgets, dispatch] = useReducer(WidgetReducer, [])\n\n  useSocketWidgetsInitEvent(screenId, (newWidgets) => {\n    console.log('update all widgets', newWidgets)\n    dispatch({\n      type: 'set',\n      payload: newWidgets\n    })\n    setLoaded(true)\n  })\n\n  useSocketWidgetUpdateEvent(screenId, (freshWidget) => {\n    console.log('Update single widget', freshWidget)\n    dispatch({\n      type: 'update',\n      payload: freshWidget\n    })\n  })\n\n  const onUpdateLayout = useCallback((newLayout : Array<any>) => {\n    io.emit(`screens:update-layout`, { layout: newLayout, screenId })\n  }, [io, screenId, widgets])\n\n  useLayoutEffect(() => {\n    setLoaded(false)\n  }, [screenId])\n\n  return [widgets, onUpdateLayout]\n}\n"},"lineCount":null}},"error":null,"hash":"71223ef00b75e3238c027ba5f9f095d9","cacheData":{"env":{}}}